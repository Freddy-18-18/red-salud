{
  "name": "red-salud-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.4.0",
  "scripts": {
    "nx": "nx",
    "dev": "pnpm --filter red-salud-web dev",
    "build": "pnpm --filter @red-salud/types build && pnpm --filter red-salud-web build",
    "start": "pnpm --filter red-salud-web start",
    "web:dev": "pnpm --filter red-salud-web dev",
    "tauri:dev": "pnpm --filter @red-salud/farmacia-desktop tauri:dev",
    "tauri:farmacia:dev": "pnpm --filter @red-salud/farmacia-desktop tauri:dev",
    "tauri:medico:dev": "pnpm --filter red-salud-desktop-medico tauri dev",
    "medico:dev": "pnpm --filter @red-salud/medico-web dev",
    "secretaria:dev": "pnpm --filter @red-salud/secretaria dev",
    "paciente:dev": "pnpm --filter @red-salud/paciente-web dev",
    "lint": "nx run red-salud-web:lint --outputStyle=static",
    "typecheck": "nx run red-salud-web:typecheck --outputStyle=static",
    "test": "nx run red-salud-web:test --outputStyle=static",
    "lint:all": "nx run-many -t lint --all --outputStyle=static",
    "check": "pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@nx/eslint-plugin": "^22.5.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^16.4.0",
    "knip": "^5.83.1",
    "next": "16.1.0",
    "nx": "22.5.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "syncpack": "^13.0.4",
    "typescript-eslint": "^8.46.1"
  }
}